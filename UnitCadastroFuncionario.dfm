  inherited Update: TIBUpdateSQL
    RefreshSQL.Strings = (
      'Select '
      '  IF_FUNC,'
      '  NOME,'
      '  CPF,'
      '  DT_NASC,'
      '  SETOR_ID_SET,'
      '  CIDADES_CID_ID,'
      '  SEXO,'
      '  TELEFONE,'
      '  EMAIL,'
      '  CARGOS_CRG'
      'from FUNCIONARIOS '
      'where'
      '  IF_FUNC = :IF_FUNC')
    ModifySQL.Strings = (
      'update FUNCIONARIOS'
      'set'
      '  IF_FUNC = :IF_FUNC,'
      '  NOME = :NOME,'
      '  CPF = :CPF,'
      '  DT_NASC = :DT_NASC,'
      '  SETOR_ID_SET = :SETOR_ID_SET,'
      '  CIDADES_CID_ID = :CIDADES_CID_ID,'
      '  SEXO = :SEXO,'
      '  TELEFONE = :TELEFONE,'
      '  EMAIL = :EMAIL,'
      '  CARGOS_CRG = :CARGOS_CRG'
      'where'
      '  IF_FUNC = :OLD_IF_FUNC')
    InsertSQL.Strings = (
      'insert into FUNCIONARIOS'
      
        '  (IF_FUNC, NOME, CPF, DT_NASC, SETOR_ID_SET, CIDADES_CID_ID, SE' +
        'XO, TELEFONE, '
      '   EMAIL, CARGOS_CRG)'
      'values'
      
        '  (:IF_FUNC, :NOME, :CPF, :DT_NASC, :SETOR_ID_SET, :CIDADES_CID_' +
        'ID, :SEXO, '
      '   :TELEFONE, :EMAIL, :CARGOS_CRG)')
    DeleteSQL.Strings = (
      'delete from FUNCIONARIOS'
      'where'
      '  IF_FUNC = :OLD_IF_FUNC')
    Left = 512
    Top = 144
  end
