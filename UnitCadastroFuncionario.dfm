  inherited Query: TIBQuery
    SQL.Strings = (
      
        'SELECT FUNCIONARIOS.*, CIDADES.NOME, SETOR.NOME, CARGOS.NOME AS ' +
        'NMCARGO'
      ' FROM FUNCIONARIOS'
      ' INNER JOIN CIDADES ON'
      ' (FUNCIONARIOS.CIDADES_CID_ID = CIDADES.CID_ID)'
      ''
      'INNER JOIN SETOR ON'
      '(FUNCIONARIOS.SETOR_ID_SET = SETOR.ID_SET)'
      ''
      'INNER JOIN CARGOS ON'
      '(FUNCIONARIOS.CARGOS_CRG = CARGOS.CRG)'
      ''
      'ORDER BY FUNCIONARIOS.IF_FUNC')
    Left = 472
    Top = 144
